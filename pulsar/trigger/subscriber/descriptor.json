{
	"name": "pulsar-subscriber",
	"type": "flogo:trigger",
	"version": "1.1.4",
	"author": "TIBCO Software Inc.",
	"title": "Apache Pulsar Consumer",
	"description": "A pulsar trigger which reads messages from apache pulsar queue",
	"settings": [
		{
			"name": "connection",
			"type": "connection",
			"required": true
		}
	],
	"output": [
		{
			"name": "payload",
			"type": "any"
		},
		{
			"name": "properties",
			"type": "object"
		},
		{
			"name": "topic",
			"type": "string"
		},
		{
			"name": "msgid",
			"type": "string"
		},
		{
			"name": "redeliveryCount",
			"type": "integer"
		}
	],
	"handler": {
		"settings": [
			{
				"name": "topic",
				"type": "string",
				"required": false
			},
			{
				"name": "topicsPattern",
				"type": "string",
				"required": false
			},
			{
				"name": "subscriptionName",
				"type": "string",
				"required": true,
				"value": ""
			},
			{
				"name": "subscriptionType",
				"type": "string",
				"required": true,
				"allowed": [
					"Exclusive",
					"Shared",
					"Failover",
					"KeyShared"
				],
				"value": "Shared"
			},
			{
				"name": "processingMode",
				"type": "string",
				"required": true,
				"allowed": [
					"Sync",
					"Async"
				],
				"value": "Sync"
			},
			{
				"name": "initialPosition",
				"type": "string",
				"required": true,
				"allowed": [
					"Latest",
					"Earliest"
				],
				"value": "Latest"
			},
			{
                "name": "seek",
                "type": "string",
                "required": false,
                "allowed": [
                    "Seek By MessageID",
                    "Seek By Timestamp"
                ],
                "value": ""
            },
			{
				"name": "seekTime",
				"type": "string",
				"required": false
			},
			{
				"name": "ledgerId",
				"type": "integer",
				"required": false,
				"value": 0
			},
			{
				"name": "entryId",
				"type": "integer",
				"required": false,
				"value": 0
			},
			
			{
				"name": "dlqTopic",
				"type": "string",
				"required": false,
				"value": ""
			},
			{
				"name": "dlqMaxDeliveries",
				"type": "integer",
				"required": false,
				"value": 3
			},
			{
				"name": "nackRedeliveryDelay",
				"type": "integer",
				"required": false,
				"value": 60
			},
			{
				"name": "enableBatchIndexAcknowledgment",
				"type": "boolean",
				"required": false,
				"value": false
			},
			{
				"name": "maxPendingChunkedMessage",
				"type": "integer",
				"required": false,
				"value": 100
			},
			{
				"name": "expireTimeOfIncompleteChunk",
				"type": "integer",
				"required": false,
				"value": 60
			},
			{
				"name": "autoAckIncompleteChunk",
				"type": "boolean",
				"required": false,
				"value": false
			}
		]
	}
}